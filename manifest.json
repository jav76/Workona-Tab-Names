{
    "manifest_version": 3,
    "name": "Workona Tab Prefix",
    "version": "1.0",
    "description": "Adds the current space name to tab names",
    "permissions": ["tabs", "activeTab", "storage", "scripting"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
  }
  